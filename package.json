{
  "name": "site",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:exp": "next build && next export",
    "start": "next start",
    "lint": "eslint src  --max-warnings=0",
    "lint:css": "stylelint './src/**/*.ts'",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore \"src/**/*.+(js|jsx|json|ts|tsx)\"",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "pre-commit-lint": "yarn check-types && yarn lint-staged",
    "commit": "yarn pre-commit-lint && cz"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn prettier --write"
    ]
  },
  "dependencies": {
    "@styled-icons/boxicons-logos": "^10.38.0",
    "@styled-icons/heroicons-outline": "^10.36.0",
    "aos": "^2.3.4",
    "axios": "^0.25.0",
    "next": "^12.1.0",
    "next-seo": "^5.1.0",
    "nextjs-progressbar": "^0.0.14",
    "plop": "^3.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3",
    "vanilla-masker": "^1.2.0"
  },
  "devDependencies": {
    "@types/aos": "^3.0.4",
    "@babel/core": "^7.17.5",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.21",
    "@types/vanilla-masker": "^1.2.1",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "babel-plugin-styled-components": "^2.0.6",
    "eslint": "^8.9.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "lint-staged": "^12.3.7",
    "prettier": "2.5.1",
    "stylelint": "^14.1.0",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.5.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "estartandos-devs",
    "frontend",
    "site"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
