{
  "name": "site",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:exp": "next build && next export",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "eslint src  --max-warnings=0",
    "stylelint": "stylelint \"**/styles.ts\" --max-warnings=0",
    "check-types": "tsc --noEmit",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "pre-commit-lint": "yarn check-types && yarn lint-staged",
    "commit": "yarn pre-commit-lint && cz"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn prettier"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@styled-icons/boxicons-logos": "^10.47.0",
    "@styled-icons/boxicons-regular": "^10.47.0",
    "@styled-icons/heroicons-outline": "^10.47.0",
    "aos": "^2.3.4",
    "axios": "^1.6.7",
    "cookies-next": "^2.1.2",
    "framer-motion": "^7.10.3",
    "html2canvas": "^1.4.1",
    "next": "14.1.3",
    "next-seo": "^5.15.0",
    "nextjs-progressbar": "^0.0.16",
    "plop": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-easy-crop": "^4.7.5",
    "react-hook-form": "^7.51.0",
    "styled-components": "^6.1.8",
    "vanilla-masker": "^1.2.0"
  },
  "devDependencies": {
    "@types/aos": "^3.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.23",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@types/styled-components": "^5.1.34",
    "@types/vanilla-masker": "^1.2.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^13.5.6",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "lint-staged": "^13.3.0",
    "next-sitemap": "^3.1.55",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.4",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "estartandos-devs",
    "frontend",
    "site"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
